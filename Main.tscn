script/source = "extends Node2D

# class member variables go here, for example:
# var a = 2
# var b = \"textvar\"

func _ready():
	pass
	# Called when the node is added to the scene for the first time.
	# Initialization here
func _process(delta):
	var platform = $TileMap
	
	var map_limits = platform.get_used_rect()
	var map_cellsize = platform.cell_size
	
	var left_limit = map_limits.position.x * map_cellsize.x
	var right_limit =  map_limits.end.x * map_cellsize.x
	var down_limit =  map_limits.end.y * map_cellsize.y
	$KinematicBody2D/Camera2D.limit_left = left_limit
	$KinematicBody2D/Camera2D.limit_right = right_limit
	$KinematicBody2D/Camera2D.limit_bottom = down_limit + 16
	
	if($KinematicBody2D.get_position().x < left_limit + 8):
		$KinematicBody2D.position.x = 8
		
	elif($KinematicBody2D.get_position().x > right_limit - 8):
		$KinematicBody2D.position.x = right_limit - 8
		
	elif($KinematicBody2D.get_position().y > down_limit):
		$KinematicBody2D.position = Vector2(-32, -8)
"

